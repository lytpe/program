<!--pages/Users/staff/staff.wxml-->
<i-collapse>
<block wx:for="{{staffs}}" wx:key="id" wx:for-index="id">
   <i-collapse-item title="{{item.userName}}" name="{{item.id}}">
      <view slot="content">
          <i-row>
            <i-col span="4">{{item.userPic}}</i-col>
             <i-col span="10"><view class="adtype">
             {{item.address}}
             </view></i-col>
            <i-col span="10">
            <view class="moneytype">
            余额：{{item.balance}}
            </view>
            </i-col>
          </i-row>
          <i-row><i-col span="24">
          {{item.userMessage}}
          </i-col></i-row>
      </view>
  </i-collapse-item>
</block>
</i-collapse>


